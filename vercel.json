{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [{
    "path": "/api/cron",
    "schedule": "0 6 * * *"
  }],
  "functions": {
    "api/cron.js": {
      "maxDuration": 300
    }
  }
}
